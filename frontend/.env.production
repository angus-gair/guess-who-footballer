# Production Environment Variables for Football Guess Who Frontend

# API URL for production
VITE_API_URL=https://who.gair.com.au/api

# WebSocket URL for production
VITE_SOCKET_URL=https://who.gair.com.au

# Environment indicator
VITE_ENV=production

# Game configuration
VITE_MAX_QUESTIONS=15
VITE_DEFAULT_TURN_TIME=30

# Feature flags
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_SOUND=true

# Analytics (if needed)
VITE_ENABLE_ANALYTICS=true

VITE_APP_NAME=Football Guess Who 